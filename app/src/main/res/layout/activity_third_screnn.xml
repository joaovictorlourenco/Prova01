<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:name=".ThirdScrenn"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <EditText
        android:id="@+id/qtd_assentosEdit"
        android:layout_width="93dp"
        android:layout_height="52dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/confirmButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/capacityTankEdit"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="191dp"
        android:layout_height="23dp"
        android:text="Quantidade de Assentos"
        app:layout_constraintBottom_toTopOf="@+id/backToscreenEdit"
        app:layout_constraintEnd_toStartOf="@+id/qtd_assentosEdit"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="191dp"
        android:layout_height="23dp"
        android:text="Capacidade do Tanque?"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toStartOf="@+id/capacityTankEdit"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/isCondicionadoEdit"
        app:layout_constraintVertical_bias="0.5" />

    <EditText
        android:id="@+id/caixaFloatEdit"
        android:layout_width="224dp"
        android:layout_height="54dp"
        android:ems="10"
        android:inputType="numberDecimal"
        app:layout_constraintBottom_toTopOf="@+id/isCondicionadoEdit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/cnpjTextEdit"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="46dp"
        android:layout_height="19dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="220dp"
        android:text="@string/nome"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="39dp"
        android:layout_height="20dp"
        android:text="CNPJ"
        app:layout_constraintBottom_toTopOf="@+id/textView9"
        app:layout_constraintEnd_toStartOf="@+id/cnpjTextEdit"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        app:layout_constraintVertical_bias="0.5" />

    <EditText
        android:id="@+id/cnpjTextEdit"
        android:layout_width="222dp"
        android:layout_height="52dp"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintBottom_toTopOf="@+id/caixaFloatEdit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/nomeTextEdit"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="39dp"
        android:layout_height="20dp"
        android:layout_marginBottom="152dp"
        android:text="Caixa"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintEnd_toStartOf="@+id/caixaFloatEdit"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/nomeTextEdit"
        android:layout_width="219dp"
        android:layout_height="58dp"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintBottom_toTopOf="@+id/cnpjTextEdit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/categoryBussinessEdit"
        app:layout_constraintVertical_bias="0.5" />

    <RadioGroup
        android:id="@+id/categoryBussinessEdit"
        android:layout_width="134dp"
        android:layout_height="144dp"
        app:layout_constraintBottom_toTopOf="@+id/nomeTextEdit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.5">

        <RadioButton
            android:id="@+id/supermercadoButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Supermercado"
            tools:layout_editor_absoluteX="95dp"
            tools:layout_editor_absoluteY="68dp" />

        <RadioButton
            android:id="@+id/postoButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Posto de Gasolina"
            tools:layout_editor_absoluteX="95dp"
            tools:layout_editor_absoluteY="20dp" />

        <RadioButton
            android:id="@+id/cinemaButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Cinema"
            tools:layout_editor_absoluteX="95dp"
            tools:layout_editor_absoluteY="116dp" />
    </RadioGroup>

    <EditText
        android:id="@+id/capacityTankEdit"
        android:layout_width="83dp"
        android:layout_height="57dp"
        android:ems="10"
        android:inputType="numberDecimal"
        app:layout_constraintBottom_toTopOf="@+id/qtd_assentosEdit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/isCondicionadoEdit"
        app:layout_constraintVertical_bias="0.5" />

    <CheckBox
        android:id="@+id/isCondicionadoEdit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tem Ar Condicionado?"
        app:layout_constraintBottom_toTopOf="@+id/capacityTankEdit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/caixaFloatEdit"
        app:layout_constraintVertical_bias="0.5" />

    <Button
        android:id="@+id/confirmButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Confirmar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/backToscreenEdit"
        app:layout_constraintTop_toBottomOf="@+id/qtd_assentosEdit"
        app:layout_constraintVertical_bias="0.5" />

    <Button
        android:id="@+id/backToscreenEdit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:text="Cancelar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/confirmButton"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>